package AllMight_Transform

import objectIDs
import LinkedList
import Transformation
import RegisterEvents

function onCast()
	if GetSpellAbilityId() == ALL_MIGHT_TRANSFORM1
		let caster = GetTriggerUnit()
		let swaps = new LinkedList<int>
		..add(ALL_MIGHT_SWAPPER1, ALL_MIGHT_SWAPPER2)
		Transformation.start(caster, ALL_MIGHT_POWER_SKIN, swaps, 10.)

init
	registerPlayerUnitEvent(EVENT_PLAYER_UNIT_SPELL_EFFECT, () -> onCast())